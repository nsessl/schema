      <xs:documentation>Hlášení z ISSD do ESSL
Zápis data externí spouštěcí události</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="SpisId" type="tIdSpisu"/>
